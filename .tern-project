{
  "ecmaVersion": 6,
  "libs": [
    "browser"
  ],
  "loadEagerly": [
    "assets/js/**/*.js",
    "node_modules/@benstyles/*/*.js",
    "node_modules/@benstyles/*/!(node_modules)/*.js"
  ],
  "plugins": {
    "complete_strings": {},
    "node": {},
    "modules": {},
    "es_modules": {},
    "doc_comment": {
      "fullDocs": true
    }
  }
}
